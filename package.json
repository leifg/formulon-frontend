{
  "name": "formulon-frontend",
  "version": "0.0.0-semantically-released",
  "description": "Frontend for Formulon (a Salesforce Formula Parser)",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "deploy": "gulp --s3Bucket=${S3_BUCKET} --awsKeyId=${AWS_KEY_ID} --awsSecretAccessKey=${AWS_SECRET_ACCESS_KEY} --cfDistributionId=${CF_DISTRIBUTION_ID} --awsRegion=${AWS_REGION} deploy",
    "semantic-release": "semantic-release pre && npm run deploy && semantic-release post"
  },
  "release": {
    "verifyConditions": "condition-circle",
    "getLastRelease": "last-release-git-tag"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifg/formulon-frontend.git"
  },
  "keywords": [
    "formulon",
    "parser",
    "salesforce"
  ],
  "author": "Leif Gensert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leifg/formulon-frontend/issues"
  },
  "homepage": "https://github.com/leifg/formulon-frontend#readme",
  "dependencies": {
    "@salesforce-ux/design-system": "^1.0.0",
    "formulon": "^2.3.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-lightning-design-system": "^0.2.10",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "svg4everybody": "git://github.com/leifg/svg4everybody.git#check-for-navigator"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "condition-circle": "^1.2.0",
    "deep-freeze": "0.0.1",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-awspublish": "^3.0.1",
    "gulp-cloudfront": "0.0.14",
    "gulp-rev-all": "^0.8.24",
    "gulp-sass": "^2.2.0",
    "gulp-sequence": "^0.4.5",
    "gulp-util": "^3.0.7",
    "install": "^0.13.0",
    "jsdom": "^8.3.0",
    "last-release-git-tag": "git://github.com/RisingStack/last-release-git-tag",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^3.2.0",
    "semantic-release": "^4.3.5",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
